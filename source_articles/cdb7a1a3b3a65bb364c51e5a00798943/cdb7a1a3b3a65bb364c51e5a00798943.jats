<name><surname>Grattarola</surname><given-names>C.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref><xref ref-type="corresp" rid="IDs41598019424746_cor1">a</xref></contrib><contrib contrib-type="author" id="Au2"><name><surname>Gallina</surname><given-names>S.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author" id="Au3"><name><surname>Giorda</surname><given-names>F.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author" id="Au4"><name><surname>Pautasso</surname><given-names>A.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author" id="Au5"><name><surname>Ballardini</surname><given-names>M.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author" id="Au6"><name><surname>Iulini</surname><given-names>B.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author" id="Au7"><name><surname>Varello</surname><given-names>K.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author" id="Au8"><name><surname>Goria</surname><given-names>M.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author" id="Au9"><name><surname>Peletto</surname><given-names>S.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author" id="Au10"><name><surname>Masoero</surname><given-names>L.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author" id="Au11"><name><surname>Serracca</surname><given-names>L.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author" id="Au12"><name><surname>Romano</surname><given-names>A.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author" id="A