				\usepackage{amssymb}
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$ A_{n}= \int _{nT}^{(n+1)T} \beta (t) S(t)I(t) \,dt, $$\end{document}</tex-math><graphic position="anchor" xlink:href="13662_2019_2339_Article_Equf.gif"/></alternatives></disp-formula> where <inline-formula id="IEq29"><alternatives><mml:math><mml:mi>β</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo><mml:mi>S</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo><mml:mi>I</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:math><tex-math id="IEq29_TeX">\documentclass[12pt]{minimal}
