	]><response><apiMessage>This XML was provided by Springer Nature</apiMessage><query>doi:10.1186/s12929-019-0542-9</query><apiKey>6b19ee21fe6a243741a528650f7c8997</apiKey><result><total>1</total><start>1</start><pageLength>1</pageLength><recordsDisplayed>1</recordsDisplayed></result><records><article dtd-version="1.2" article-type="review-article" xml:lang="en" xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink"><?properties open_access?><front><journal-meta><journal-id journal-id-type="publisher-id">12929</journal-id><journal-title-group><journal-title>Journal of Biomedical Science</journal-title><abbrev-journal-title abbrev-type="publisher">J Biomed Sci</abbrev-journal-title></journal-title-group><issn pub-type="epub">1423-0127</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="publisher-id">s12929-019-0542-9</article-id><article-id pub-id-type="manuscript">542</article-id><article-id pub-id-type="doi">10.1186/s12929-019-0542-9</article-id><article-categories><subj-group subj-group-type="heading"><subject>Review</subject></subj-group></article-categories><title-group><article-title xml:lang="en">Oncolytic paramyxoviruses-induced autophagy; a prudent weapon for cancer therapy</article-title></title-group><contrib-group><contrib contrib-type="author" id="Au1"><name><surname>Keshavarz</surname><given-names>Mohsen</given-names></name><xref ref-type="aff" rid="Aff1">1</xref><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author" id="Au2"><name><surname>Solaymani-Mohammadi</surname><given-names>Farid</given-names></name><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author" id="Au3"><name><surname>Miri</surname><given-names>Seyed Mohammad</given-names></name><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author" corresp="yes" id="Au4"><name><s