-type="author" id="Au12"><name><surname>Baldet</surname><given-names>Thierry</given-names></name><xref ref-type="aff" rid="Aff8">8</xref><xref ref-type="aff" rid="Aff9">9</xref></contrib><contrib contrib-type="author" id="Au13"><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6755-6167</contrib-id><name><surname>Moiroux</surname><given-names>Nicolas</given-names></name><xref ref-type="aff" rid="Aff8">8</xref><xref ref-type="aff" rid="Aff9">9</xref></contrib><contrib contrib-type="author" id="Au14"><name><surname>Carosella</surname><given-names>Edgardo</given-names></name><xref ref-type="aff" rid="Aff6">6</xref><xref ref-type="aff" rid="Aff7">7</xref></contrib><contrib contrib-type="author" id="Au15"><name><surname>Moreau</surname><given-names>Philippe</given-names></name><xref ref-type="aff" rid="Aff6">6</xref><xref ref-type="aff" rid="Aff7">7</xref></contrib><contrib contrib-type="author" id="Au16"><name><surname>Rouas-Freiss</surname><given-names>Nathalie</given-names></name><xref ref-type="aff" rid="Aff6">6</xref><xref ref-type="aff" rid="Aff7">7</xref></contrib><contrib contrib-type="author" id="Au17"><name><surname>Cottrell</surname><given-names>Gilles</given-names></name><xref ref-type="aff" rid="Aff2">2</xref><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author" id="Au18"><name><surname>Courtin</surname><given-names>David</given-names></name><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author" corresp="yes" id="Au19"><name><surname>Garcia</surname><given-names>André</given-names></name><xref ref-type="aff" rid="Aff2">2</xref><xref ref-type="aff" rid="Aff3">3</xref><xref ref-type="corresp" rid="IDs4159801945688w_cor19">v</xref></contrib><aff id="Aff1"><label>1</label><institution-wrap><institution-id institution-id-type="ISNI">0000 0001 2308 1657</institution-id><institution-id institution-id-type="GRID">grid.462844.8</institution-id><institution content-type="org-name">Université Pierre et M