	]><response><apiMessage>This XML was provided by Springer Nature</apiMessage><query>doi:10.1007/s10719-020-09944-w</query><apiKey>6b19ee21fe6a243741a528650f7c8997</apiKey><result><total>1</total><start>1</start><pageLength>1</pageLength><recordsDisplayed>1</recordsDisplayed></result><records><article dtd-version="1.2" article-type="research-article" xml:lang="en" xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink"><?properties open_access?><front><journal-meta><journal-id journal-id-type="publisher-id">10719</journal-id><journal-id journal-id-type="doi">10.1007/10719.1573-4986</journal-id><journal-title-group><journal-title>Glycoconjugate Journal</journal-title><journal-subtitle>Official Journal of the International Glycoconjugate Organization</journal-subtitle><abbrev-journal-title abbrev-type="publisher">Glycoconj J</abbrev-journal-title></journal-title-group><issn pub-type="ppub">0282-0080</issn><issn pub-type="epub">1573-4986</issn><publisher><publisher-name>Springer US</publisher-name><publisher-loc>New York</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="publisher-id">s10719-020-09944-w</article-id><article-id pub-id-type="manuscript">9944</article-id><article-id pub-id-type="doi">10.1007/s10719-020-09944-w</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Article</subject></subj-group></article-categories><title-group><article-title xml:lang="en">APOL1 polymorphism modulates sphingolipid profile of human podocytes</article-title></title-group><contrib-group><contrib contrib-type="author" id="Au1"><name><surname>Valsecchi</surname><given-names>Manuela</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author" id="Au2"><name><surname>Cazzetta</surname><given-names>Valentina</given-names></name><xref ref-type="aff" rid="Aff1">1</xref><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author"